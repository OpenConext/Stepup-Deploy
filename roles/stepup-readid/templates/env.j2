KERNEL_CLASS='StepupReadId\Kernel'
APP_ENV=prod

# App secret for CSRF protection
APP_SECRET={{ readid_secret | vault(vault_keydir) }}

# Requires SUBMITTER rights
APP_SUBMITTER_TOKEN="{{ readid_submitter_token | vault(vault_keydir) }}"

# Requires VIEWER rights (TODO: and MANAGER?)
APP_VIEWER_TOKEN="{{ readid_viewer_token | vault(vault_keydir) }}"

# TODO: What is this?
APP_OPAQUE_ID="{{ readid_opaque_id | vault(vault_keydir) }}"

APP_READID_SERVER_FQDN=saas-preprod.readid.com

# Generated by us
# Configure webhook in readID like https://<read vhost name>/webhook/<APP_WEBHOOK_TOKEN>
APP_WEBHOOK_TOKEN={{ readid_webhook_token | vault(vault_keydir) }}
# TODO: What attrbuties ae available?
APP_ATTRIBUTES_MAPPING='{}'
